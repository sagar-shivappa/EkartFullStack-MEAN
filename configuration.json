{
  "version": "1.0",
  "default_open_files": [
    "backend/controllers/ekart.controller.js",
    "backend/routes/ekart.route.js",
    "frontend/src/app/services/http-service.service.ts",
    "frontend/src/app/components/login/login.component.html",
    "frontend/src/app/components/login/login.component.ts",
    "frontend/src/app/components/home/home.component.html",
    "frontend/src/app/components/home/home.component.ts"
  ],
  "read_only_files": [
    "backend/tests/cart.test.js",
    "backend/tests/login.test.js",
    "backend/tests/products.test.js",
    "frontend/src/app/app.component.spec.ts",
    "frontend/src/app/components/header/header.component.spec.ts",
    "frontend/src/app/components/home/home.component.spec.ts",
    "frontend/src/app/components/home/user-cart/user-cart.component.spec.ts",
    "frontend/src/app/components/login/login.component.spec.ts",
    "frontend/src/app/gaurd/auth.guard.spec.ts",
    "frontend/src/app/Interceptor/auth.interceptor.spec.ts",
    "frontend/src/app/services/auth.services.spec.ts",
    ".gitignore",
    "configuration.json"
  ],
  "buttons": {
    "Install Packages": {
      "command": "npm install"
    },
    "Start MongoDB": {
      "command": "mongod --dbpath /var/lib/mongo --logpath /var/log/mongodb/mongod.log"
    },
    "Start Frontend Dev Server": {
      "command": "npm run client"
    },
    "Start Backend API Server": {
      "command": "npm run server"
    },
    "Concurrently Start Both Servers": {
      "command": "npm start"
    },
    "Run Frontend Test Cases": {
      "command": "npm run testclient"
    },
    "Run Backend Test Cases": {
      "command": "npm run testserver"
    },
    "Run All Test Cases": {
      "command": "npm test"
    }
  },
  "scoring": {
    "command": "npm install && npm run test",
    "files": {
      "frontend/junit.xml": "Angular",
      "backend/junit.xml": "Node & Express"
    }
  },
  "post_start": {
    "commands": [
      "npm install",
      "mongod --dbpath /var/lib/mongo --logpath /var/log/mongodb/mongod.log"
    ]
  },
  "preview_port": 8000
}
